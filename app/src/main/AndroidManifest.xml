<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginregister">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.user.MoneyUpi"></activity>
        <activity android:name="com.example.user.profile" />
        <activity android:name="com.example.user.Thankyou" />
        <activity android:name="com.example.user.Print_details" />
        <activity android:name="com.example.user.Food_details" />
        <activity android:name="com.example.Common.Donate_money" />
        <activity android:name="com.example.Common.Contact" />
        <activity android:name=".Login" />
        <activity android:name="com.example.Common.Donatefood" />
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name="com.example.user.dashboard" />
        <activity android:name="com.example.Common.About" />
    </application>

</manifest>